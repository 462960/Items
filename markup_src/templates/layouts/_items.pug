.wrap-inventory-block
  .wrap-top-row-colaborator.posr
    .tooltip-items-lime
      .tooltip-items-lime-left
        .tooltip-items-lime-left-item.m-r-30.bold 3 Items selectors
        .tooltip-items-lime-left-item.m-r-15.bold Change:
        .tooltip-items-lime-left-item.m-r-15 Collaborator
        .tooltip-items-lime-left-item.m-r-15 Status
        .tooltip-items-lime-left-item.m-r-15 Location
      .tooltip-items-lime-right
        .tooltip-items-lime-right-item.m-r-15
          span.title-header-btn Import
          svg.svg-down-arrow: use(xlink:href="/images/sprite.svg#down-arrow")
        .tooltip-items-lime-right-item.m-r-15 Archive
        .tooltip-items-lime-right-item.m-r-15 Delete
    .wrap-df.posr
      input(type='text',placeholder='Searsh...').input-inventory.m-r-10
      img(src="/images/small-lupe.svg" class="lupe-abs" alt="page")
      button.btn.btn-inventor.btn-white
        //img(src="/images/page.svg" alt="page").m-r-10
        svg.svg-bookmark: use(xlink:href="/images/sprite.svg#bookmark")
        span.title-header-btn.m-r-10.m-l-10 Import
        img(src="/images/arrow-down.svg" alt="arrow")
    .wrap-main-tooltip.posr
      button.wrap-row.wrap-row-hov
        svg.svg-gear: use(xlink:href="/images/sprite.svg#gear-new").m-r-10
        //img(src="/images/gear.png" class="gear" alt="gear-icon").m-r-10
        span.custom-text Customize columns
      .tooltip-items
        .tooltip-items-header
          .tooltip-items-header-title Show columns
          .tooltip-items-header-close
            span.m-r-10 Close
            button.style-btn-non
              svg.svg-cancel: use(xlink:href="/images/sprite.svg#delete")
        .tooltip-items-content
          ul
            li
